{
    "name": "unusualify/laravel-form",
    "type": "library",
    "license": "MIT",
    "description": "Form input components for laravel blade",
    "keywords": [
        "template",
        "form",
        "laravel"
    ],
    "authors": [
        {
            "name": "Oğuzhan Bükçüoğlu",
            "email": "oguz.bukcuoglu@gmail.com"
        },
        {
            "name": "Güneş Bizim",
            "email": "gnsbzm@gmail.com"
        },
        {
            "name": "Hazarcan Doğa Bakan",
            "email": "doga@olmadikprojeler.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "cartalyst/tags": "^6.0|^7.0|^8.0|^9.0|^10.0|^11.0|^12.0|^13.0",
        "intervention/image": "^2.7"
    },
    "require-dev": {},
    "extra": {
        "laravel": {
            "providers": [
                "Unusualify\\LaravelForm\\Providers\\UnusualServiceProvider"
            ],
            "aliases": {

            }
